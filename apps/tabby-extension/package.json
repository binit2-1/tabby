{
  "name": "tabby-clipboard",
  "displayName": "Tabby Cloud clipboard",
  "description": "Sync code snippets from web",
  "version": "0.0.3",
  "publisher": "binit-dev",
  "icon": "images/vscode-ext.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binit2-1/tabby.git",
    "directory": "apps/tabby-extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tabby.connect",
        "title": "Tabby: Connect Bundle"
      },
      {
        "command": "tabby.refresh",
        "title": "Tabby: Refresh Snippets"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@upstash/redis": "^1.36.1",
    "axios": "^1.0.0"
  }
}